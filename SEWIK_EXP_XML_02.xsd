<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:sewik="http://www.policja.gov.pl/sewik" targetNamespace="http://www.policja.gov.pl/sewik" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="2.4">
	<!-- Definicja schematu zrzutu SEWIK dla podmiotów zewnętrznych
    -->
	<!--++++++++++++ Typy proste ++++++++++++++-->
	<xsd:simpleType name="ZSZD_ID">
		<xsd:annotation>
			<xsd:documentation>Identyfikator zdarzenia drogowego</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:totalDigits value="38"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ZSUC_ID">
		<xsd:annotation>
			<xsd:documentation>Identyfikator uczestnika zdarzenia</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:totalDigits value="38"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ZSPO_ID">
		<xsd:annotation>
			<xsd:documentation>Identyfikator pojazdu uczestniczącego w zdarzeniu</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:totalDigits value="38"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DICT_KOD">
		<xsd:annotation>
			<xsd:documentation>Typ wartości kodu słownika</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DICT_TYPE">
		<xsd:annotation>
			<xsd:documentation>Typ wartości domeny słownika</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SKAR_KOD">
		<xsd:annotation>
			<xsd:documentation>Rodzaj pojazdu ze słownika </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text1">
		<xsd:annotation>
			<xsd:documentation>Typ String 1 znak</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text255">
		<xsd:annotation>
			<xsd:documentation>Typ String 255 znaków</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="KodGUS">
		<xsd:annotation>
			<xsd:documentation>Typ String 30 znaków kodu GUS</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="GPSType">
		<xsd:annotation>
			<xsd:documentation>Typ danych GPS</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="9"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="GPSType2">
		<xsd:annotation>
			<xsd:documentation>Typ danych GPS wg KOD GUS wzorzec 99,9999999999999</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="17"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Text2000">
		<xsd:annotation>
			<xsd:documentation>Typ String 2000 znaków</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--+++++++++++Typy złozone +++++++++++++++-->
	<xsd:complexType name="SPSP_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSPO_ID" type="sewik:ZSPO_ID"/>
			<xsd:element name="SPSP_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SPSP_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SPSP"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SPIC_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSPO_ID" type="sewik:ZSPO_ID"/>
