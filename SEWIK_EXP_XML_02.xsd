				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="STNA"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RODR_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="RODR_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="RODR_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="RODR"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SYSW_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="SYSW_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SYSW_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SYSW"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OZPO_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="OZPO_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="OZPO_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="OZPO"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ZABU_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="ZABU_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="ZABU_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="ZABU"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CHMZ_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="CHMZ_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="CHMZ_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE"/>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GEOD_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="GEOD_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="GEOD_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="GEOD"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SKRZ_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="SKRZ_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SKRZ_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SKRZ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SSWA_KODType">
		<xsd:sequence minOccurs="1" maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="SSWA_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SSWA_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SSWA"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SSUP_KODType">
		<xsd:sequence minOccurs="1" maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="SSUP_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SSUP_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SSUP"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SPIP_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID"/>
			<xsd:element name="SPIP_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SPIP_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SPIP"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUSB_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSUC_ID" type="sewik:ZSUC_ID"/>
			<xsd:element name="SUSB_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SUSB_TABK_TYPE" type="sewik:DICT_TYPE"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUSW_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSUC_ID" type="sewik:ZSUC_ID"/>
			<xsd:element name="SUSW_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SUSW_TABK_TYPE" type="sewik:DICT_TYPE"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SPSZ_KODType">
		<xsd:sequence maxOccurs="1">
			<xsd:element name="ZSUC_ID" type="sewik:ZSUC_ID"/>
			<xsd:element name="SPSZ_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SPSZ_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SPSZ"/>
					</xsd:restriction>
