	<!--++++++++++++++++++++++++++-->
	<xsd:complexType name="UCZESTNICY">
		<xsd:sequence>
			<xsd:element name="OSOBA" type="sewik:OSOBAType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OSOBAType">
		<xsd:annotation>
			<xsd:documentation>Typ zawierający dane o uczestnikach</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ID" type="sewik:ZSUC_ID">
				<xsd:annotation>
					<xsd:documentation>Identyfikator uczestnka zdarzenia</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ZSZD_ID" type="sewik:ZSZD_ID">
				<xsd:annotation>
					<xsd:documentation>Identyfikator Zdarzenia drogowego</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ZSPO_ID" type="sewik:ZSPO_ID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identyfiaktor pojazdu</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SSRU_KOD" type="sewik:DICT_KOD">
				<xsd:annotation>
					<xsd:documentation>Rodzaj uczestnika: Słownik SSRU</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SSRU_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SSRU"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DATA_UR" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Data urodzenia uczestnika</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SOBY_KOD" type="sewik:DICT_KOD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Obywatelstwo uczestnika: Słownik SOBY</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SOBY_TABK_TYPE" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SOBY"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PLEC" type="sewik:Text1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Płeć uczestnika: </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MIEJSCE_W_POJ" type="sewik:Text1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Miejsce w pojeżdzie</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SRUZ_KOD" type="sewik:DICT_KOD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Rozstrzygnięcie: Słownik SRUZ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SRUZ_TABK_TYPE" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SRUZ"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="INWALIDA" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Czy uczestnik jest inwalidą ?</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SUSU_KOD" type="sewik:DICT_KOD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Czy posiada uprawnienia do kierowania? Słownik SUSU</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SUSU_TABK_TYPE" type="sewik:DICT_TYPE" minOccurs="0"/>
			<xsd:element name="LICZBA_LAT_KIEROWANIA" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Liczba Lat kierowania pojazdem</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:totalDigits value="10"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="OBCOKRAJOWIEC" type="sewik:Text1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Czy uczestnik jest obcokrajowcem ?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="STUC_KOD" type="sewik:DICT_KOD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Odnesione obrażenia: Słownik STUC</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="STUC_TABK_TYPE" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="STUC"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="INFO_DODATKOWE" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="POD_WPLYWEM" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence minOccurs="0" maxOccurs="unbounded">
									<xsd:element name="SUSW_KOD" type="sewik:SUSW_KODType"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="JAZDA_BEZ" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence minOccurs="0" maxOccurs="unbounded">
									<xsd:element name="SUSB_KOD" type="sewik:SUSB_KODType"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="SUZZ_KOD" type="sewik:DICT_KOD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Miejsce zamieszkania uczestnika: Słownik SUZZ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SUZZ_TABK_TYPE" type="sewik:DICT_TYPE" minOccurs="0"/>
			<xsd:element name="ZBIEGL_Z_MIEJSCA" type="sewik:Text1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Czy uczestnik zbiegł z miesca zdarzenia ?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="POZIOM_ALKOHOLU" type="xsd:double" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Od 29.05.2013: Poziom alkoholu </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPAK_KOD" type="sewik:DICT_KOD" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Od 29.05.2013: Jednostka miary poziomu alkoholu</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPAK_TABK_TYPE" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Od 29.05.2013</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
						<xsd:enumeration value="SPAK"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ODBLASKI" type="sewik:Text1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Od 11.2015 Czy uczestnik maił odblaski?</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PRZYCZYNY_KIEROWCY" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence minOccurs="1" maxOccurs="unbounded">
						<xsd:element name="SPSZ_KOD" type="sewik:SPSZ_KODType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PRZYCZYNY_PIESZY" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence minOccurs="1" maxOccurs="unbounded">
						<xsd:element name="SPPI_KOD" type="sewik:SPPI_KODType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="DataUtworzenia" type="xsd:date" use="required">
			<xsd:annotation>
				<xsd:documentation>Data utworzenia rekordu</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
