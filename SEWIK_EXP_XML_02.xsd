			<xsd:element name="ZSUC_ID" type="sewik:ZSUC_ID"/>
			<xsd:element name="SPPI_KOD" type="sewik:DICT_KOD"/>
			<xsd:element name="SPPI_TABK_TYPE">
				<xsd:simpleType>
					<xsd:restriction base="sewik:DICT_TYPE">
